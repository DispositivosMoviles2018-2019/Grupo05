<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uce.edu.ec.myapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="uce.edu.ec.vista.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="uce.edu.ec.vista.RegistroUsuario" />
        <activity android:name="uce.edu.ec.vista.RegistroVehiculo" />
        <activity android:name="uce.edu.ec.vista.ListaVehiculos" />
        <activity android:name="uce.edu.ec.vista.EditarVehiculo" />
        <activity android:name="uce.edu.ec.vista.IformacionVehiculo"></activity>
    </application>

</manifest>